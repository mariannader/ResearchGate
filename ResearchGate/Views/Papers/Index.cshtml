@model IEnumerable<ResearchGate.Models.tbl_paper>


@{ ViewBag.Title = "Index";
    string user = ViewBag.user;
    Layout = "~/Views/Shared/_Layout1.cshtml";}

<h2>Papers</h2>


<h3 style="text-align:center; color:dodgerblue">Hello @user !</h3>

<p>
    @Html.ActionLink("Create New Paper", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @*Html.DisplayNameFor(model => model.p_name)*@Paper Name
        </th>
        <th>
            @*Html.DisplayNameFor(model => model.p_file)*@Paper File
        </th>
        <th>
            @*Html.DisplayNameFor(model => model.tbl_user.u_name)*@Paper Author
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {


        if (item.tbl_user.u_name == user)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.p_name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.p_file)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.tbl_user.u_name)
    </td>

    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.p_id }) |
        @Html.ActionLink("Details", "Details", new { id = item.p_id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.p_id })
    </td>
</tr>
}

}
</table>